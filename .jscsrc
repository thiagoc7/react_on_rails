{
  "preset": "airbnb",
  "fileExtensions": [
    ".js",
    ".jsx"
  ],
  "excludeFiles": [
    "**/build/**",
    "**/node_modules/**",
    "**/generated/**",
    "**/docs/**",
    "**/tmp/**",
    "**/sample_generated/**",
    "**/coverage/**",
    "**/vendor/**",
    "**/dummy-for-generators/**"
  ],
  "esprima": "babel-jscs",
  "validateQuoteMarks": {
    "mark": "'",
    "escape": true,
    "ignoreJSX": true
  }
}
